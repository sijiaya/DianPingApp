<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vedioapp">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.FoodDescActivity"></activity>
        <activity android:name=".activity.GlassActivity" />
        <activity android:name=".activity.MyRegisterActivity" />
        <activity android:name=".activity.AllCategoryActivity" />
        <activity android:name=".activity.MyLoginActivity" />
        <activity android:name=".activity.GuideActivity" />
        <activity android:name=".activity.WelcomeStartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MainActivity" />
    </application>

</manifest>